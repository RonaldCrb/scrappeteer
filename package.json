{
  "name": "scrappetteer",
  "version": "1.0.0",
  "description": "Scrapper with puppetteer + express + es6",
  "private": true,
  "scripts": {
    "scrape": "node_modules/.bin/nodemon main.js --exec babel-node --",
    "build": "babel main.js --out-dir build",
    "scrape": "babel-watch main.js"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "moment": "^2.24.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "puppeteer": "^1.14.0",
    "sequelize": "^4.42.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.8",
    "nodemon": "^1.18.9"
  },
  "main": "main.js",
  "author": "Ron Alonzo",
  "license": "ISC"
}
